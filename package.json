{
  "name": "cartpilot",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "description": "CartPilot - Your intelligent in-store navigation companion",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "cap:build": "npm run build && npx cap sync",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android",
    "videos:generate": "node scripts/generate-walkthrough-videos.js",
    "videos:serve": "npx http-server videos -p 3001 -o",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:mobile": "npx playwright test --project='Mobile Chrome' --project='Mobile Safari' --project='Android Chrome'",
    "test:e2e:desktop": "npx playwright test --project='chromium' --project='firefox' --project='webkit'",
    "test:e2e:critical": "npx playwright test tests/e2e/01-user-onboarding.e2e.ts tests/e2e/02-store-search-selection.e2e.ts tests/e2e/05-shopping-cart.e2e.ts",
    "test:visual": "npx playwright test tests/visual-regression/",
    "test:performance": "npx playwright test tests/performance/",
    "lighthouse": "npx lighthouse-ci autorun",
    "test:all": "npm run test:run && npm run test:e2e && npm run lighthouse",
    "subagents:start": "node tests/subagents/runner.js",
    "subagents:config": "node tests/subagents/runner.js tests/subagents/config.json"
  },
  "dependencies": {
